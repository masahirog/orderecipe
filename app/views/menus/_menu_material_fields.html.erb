<div class="nested-fields form-inline">
  <%= f.collection_select(:material_id, Material.all, :id, :material_name) %>

  <div class="form-group">
    <%= f.label :amount_used %>
    <%= f.number_field :amount_used, step: '0.1', min: '0.0', required: true, :class => 'form-control' %>
  </div>
  <%= link_to_remove_association "remove material", f %>
</div>
